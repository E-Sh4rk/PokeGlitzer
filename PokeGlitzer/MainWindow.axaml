<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PokeGlitzer.Converters"
        mc:Ignorable="d" d:DesignWidth="410" d:DesignHeight="550"
        x:Class="PokeGlitzer.MainWindow"
        Title="PokeGlitzer">
  
  <StackPanel Orientation="Vertical">
    <Button Command="{Binding OpenPokemonInterpreted}">Open Pokemon Interpreted Data Editor!</Button>
    <Button Command="{Binding OpenPokemonData}">Open Pokemon Data Editor!</Button>
    <Button Command="{Binding OpenPokemonRaw}">Open Pokemon Raw Data Editor!</Button>
  </StackPanel>
  
</Window>
